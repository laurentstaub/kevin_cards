{
  "name": "00_kevin_cards",
  "version": "1.0.0",
  "description": "## Description Ce projet est une application web de cartes mémoire (flashcards) conçue spécifiquement pour les pharmaciens. Elle comprend un système de cartes de base avec un ensemble fixe de questions et réponses en français.",
  "main": "src/js/scripts.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node server.js",
    "start": "node server.js",
    "api": "nodemon api/server.js",
    "db:setup": "psql -d flashpharma -f database/schema.sql",
    "db:reset": "dropdb --if-exists flashpharma && createdb flashpharma && npm run db:setup",
    "migrate:preview": "node database/migrate-questions.js",
    "migrate:execute": "node database/migrate-questions.js --execute",
    "load:questions": "node load_questions.js",
    "load:db": "node load_questions_to_db.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurentstaub/kevin_cards.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/laurentstaub/kevin_cards/issues"
  },
  "homepage": "https://github.com/laurentstaub/kevin_cards#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.5",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "marked": "^9.1.6",
    "marked-highlight": "^2.2.2",
    "pg": "^8.11.3",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "live-server": "^1.2.0",
    "nodemon": "^3.0.2"
  }
}
